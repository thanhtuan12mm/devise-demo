<div id='profiles-show-container'>
  <div class='container'>
    <h1>My Profile</h1>
    <div class="card">
      <div class="card-body">
        <div><%= current_user.email %></div>
        <div><%= current_user.created_at %></div>
        <div><%= current_user.updated_at %></div>
        <%= link_to 'Logout', destroy_user_session_path, method: :delete, class: 'btn btn-danger' %>
      </div>
    </div>
  </div>
</div>
